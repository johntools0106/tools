{
  "files.autoSave": "afterDelay",
  "workbench.enableExperiments": false,
  "files.autoSaveDelay": 500,
  "editor.mouseWheelZoom": true,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.quickSuggestions": {
    "comments": "on",
    "strings": "on",
    "other": "on"
  },
  "editor.renderLineHighlightOnlyWhenFocus": true,
  "editor.multiCursorPaste": "full",
  "files.trimFinalNewlines": true,
  "workbench.list.smoothScrolling": true,
  "search.showLineNumbers": true,
  "debug.allowBreakpointsEverywhere": true,
  "git.showPushSuccessNotification": true,
  "git.supportCancellation": true,
  "code-runner.executorMap": {
    "javascript": "node",
    "java": "cd $dir && javac $fileName && java $fileNameWithoutExt",
    "c": "cd $dir && gcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "zig": "zig run",
    "cpp": "cd $dir && g++ $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "objective-c": "cd $dir && gcc -framework Cocoa $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "php": "php",
    "python": "python3 -u",
    "perl": "perl",
    "perl6": "perl6",
    "ruby": "ruby",
    "go": "go run",
    "lua": "lua",
    "groovy": "groovy",
    "powershell": "powershell -ExecutionPolicy ByPass -File",
    "bat": "cmd /c",
    "shellscript": "bash",
    "fsharp": "fsi",
    "csharp": "scriptcs",
    "vbscript": "cscript //Nologo",
    "typescript": "ts-node",
    "coffeescript": "coffee",
    "scala": "scala",
    "swift": "swift",
    "julia": "julia",
    "crystal": "crystal",
    "ocaml": "ocaml",
    "r": "Rscript",
    "applescript": "osascript",
    "clojure": "lein exec",
    "haxe": "haxe --cwd $dirWithoutTrailingSlash --run $fileNameWithoutExt",
    "rust": "cd $dir && rustc $fileName && $dir$fileNameWithoutExt",
    "racket": "racket",
    "scheme": "csi -script",
    "ahk": "autohotkey",
    "autoit": "autoit3",
    "dart": "dart",
    "pascal": "cd $dir && fpc $fileName && $dir$fileNameWithoutExt",
    "d": "cd $dir && dmd $fileName && $dir$fileNameWithoutExt",
    "haskell": "runghc",
    "nim": "nim compile --verbosity:0 --hints:off --run",
    "lisp": "sbcl --script",
    "kit": "kitc --run",
    "v": "v run",
    "sass": "sass --style expanded",
    "scss": "scss --style expanded",
    "less": "cd $dir && lessc $fileName $fileNameWithoutExt.css",
    "FortranFreeForm": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "fortran-modern": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "fortran_fixed-form": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "fortran": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "sml": "cd $dir && sml $fileName"
  },
  "code-runner.runInTerminal": true,
  "editor.definitionLinkOpensInPeek": true,
  "files.autoGuessEncoding": true,
  "workbench.editor.scrollToSwitchTabs": true,
  "workbench.editor.splitSizing": "split",
  "window.closeWhenEmpty": true,
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.cursorBlinking": true,
  "merge-conflict.autoNavigateNextConflict.enabled": true,
  "python.terminal.activateEnvInCurrentTerminal": true,
  "code-runner.clearPreviousOutput": true,
  "code-runner.saveAllFilesBeforeRun": true,
  "code-runner.saveFileBeforeRun": true,
  "editor.suggestSelection": "first",
  "editor.wordWrap": "on",
  "editor.largeFileOptimizations": false,
  "html.format.indentInnerHtml": true,
  "python.linting.flake8Enabled": true,
  "workbench.preferredDarkColorTheme": "Default Light+",
  "indentRainbow.indicatorStyle": "light",
  "indentRainbow.lightIndicatorStyleLineWidth": 5,
  // the same colors as above but more visible
  "indentRainbow.colors": [
    "rgba(255,255,64,0.3)",
    "rgba(127,255,127,0.3)",
    "rgba(255,127,255,0.3)",
    "rgba(79,236,236,0.3)"
  ],
  "workbench.colorCustomizations": {
    "[Monokai]": {
      "terminal.foreground": "#00ff62",
      "peekViewTitleLabel.foreground": "#ff0000",
      "peekViewEditor.background": "#200202",
      "list.focusBackground": "#035244",
      "editor.lineHighlightBackground": "#021a1d",
      "editor.background": "#0b192457",
      "sideBar.background": "#110c0757",
      "editor.selectionBackground": "#2a718683",
      "editor.lineHighlightBorder": "#393a6e",
      "editorGroupHeader.tabsBackground": "#3d3936",
      "editorCursor.foreground": "#0ebb50",
      "peekViewEditor.matchHighlightBackground": "#141f94",
      "tab.activeBackground": "#7a7c78a6",
      "breadcrumb.background": "#7a7c78a6",
      "editorLineNumber.activeForeground": "#88ff00e7",
      "editorLineNumber.foreground": "#00c3ff5e",
      "editor.selectionHighlightBackground": "#3331bb",
    },
  },
  "editor.tokenColorCustomizations": {
    "[Monokai]": {
      "functions": "#67cc67",
      "comments": "#a0c1c5",
      "keywords": "#ff00f2",
      "numbers": "#00ff9d",
      "strings": "#aad4d1",
      "variables": "#002fff",
      "textMateRules": [
        {
          "scope": "source.python",
          "settings": {
            "foreground": "#7e90e4",
            "fontStyle": "italic bold"
          }
        },
      ],
    },
  },
  "editor.snippetSuggestions": "bottom",
  "editor.suggest.showSnippets": true,
  "html.format.templating": true,
  "notebook.displayOrder": [],
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "editor.fontFamily": "Inconsolata,Consolas, 'Courier New', monospace",
  "editor.bracketPairColorization.enabled": true,
  "editor.suggest.preview": true,
  "workbench.startupEditor": "none",
  "jupyter.askForKernelRestart": false,
  "search.followSymlinks": false,
  "liveServer.settings.donotVerifyTags": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "indentRainbow.ignoreLinePatterns": [
    "/[ \t]* [*]/g",
    "/[ \t]+[/]{2}/g"
  ],
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.links": false,
  "files.trimTrailingWhitespace": true,
  "debug.terminal.clearBeforeReusing": true,
  "typescript.suggest.completeFunctionCalls": true,
  "typescript.format.placeOpenBraceOnNewLineForFunctions": true,
  "typescript.format.placeOpenBraceOnNewLineForControlBlocks": true,
  "typescript.format.insertSpaceBeforeFunctionParenthesis": true,
  "references.preferredLocation": "view",
  "indentRainbow.colorOnWhiteSpaceOnly": true,
  "html.format.indentHandlebars": true,
  "terminal.integrated.defaultProfile.windows": "Command Prompt",
  "editor.guides.bracketPairs": "active",
  "editor.renderControlCharacters": false,
  "editor.renderWhitespace": "boundary",
  "workbench.colorTheme": "Monokai",
  "editor.codeActionsOnSave": {},
  "workbench.tree.indent": 20,
  "editor.wrappingIndent": "indent",
  "workbench.tree.renderIndentGuides": "always",
  "terminal.integrated.inheritEnv": false,
  "python.condaPath": "conda acvivate py36",
  "EnglishChineseDictionary.enableHover": true,
  "editor.tabCompletion": "on",
  "editor.autoIndent": "advanced",
  "editor.unfoldOnClickAfterEndOfLine": true,
  "outline.collapseItems": "alwaysCollapse",
  "workbench.settings.useSplitJSON": true,
  "workbench.iconTheme": "vscode-icons",
  "editor.autoClosingDelete": "always",
  "editor.autoClosingOvertype": "always",
  "editor.defaultFormatter": "ms-python.python",
  "editor.minimap.autohide": true,
  "python.languageServer": "Jedi",
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "tabnine.experimentalAutoImports": true,
  "tabnine.debounceMilliseconds": 1,
  "tabnine.receiveBetaChannelUpdates": true,
  "codeium.enableSearch": true,
  "editor.inlineSuggest.showToolbar": "always",
  "[xslt]": {
    "editor.semanticHighlighting.enabled": true
  },
  "cmake.configureOnOpen": true,
}
