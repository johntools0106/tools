{
  "telemetry.telemetryLevel": "off",
  "workbench.enableExperiments": false,
  "files.autoSaveDelay": 500,
  "editor.mouseWheelZoom": true,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.quickSuggestions": {
    "comments": "on",
    "strings": "on",
    "other": "on"
  },
  "files.autoSave": "afterDelay",
  "editor.renderLineHighlightOnlyWhenFocus": true,
  "editor.multiCursorPaste": "full",
  "files.trimFinalNewlines": true,
  "workbench.list.smoothScrolling": true,
  "search.showLineNumbers": true,
  "debug.allowBreakpointsEverywhere": true,
  "git.showPushSuccessNotification": true,
  "git.supportCancellation": true,
  "code-runner.executorMap": {
    "python": "python3 -u"
  },
  "code-runner.runInTerminal": true,
  "editor.definitionLinkOpensInPeek": true,
  "files.autoGuessEncoding": true,
  "workbench.editor.scrollToSwitchTabs": true,
  "workbench.editor.splitSizing": "split",
  "window.closeWhenEmpty": true,
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.cursorBlinking": true,
  "merge-conflict.autoNavigateNextConflict.enabled": true,
  "python.terminal.activateEnvInCurrentTerminal": true,
  "code-runner.clearPreviousOutput": true,
  "code-runner.saveAllFilesBeforeRun": true,
  "code-runner.saveFileBeforeRun": true,
  "editor.suggestSelection": "first",
  "editor.wordWrap": "on",
  "editor.largeFileOptimizations": false,
  "html.format.indentInnerHtml": true,
  "python.linting.flake8Enabled": true,
  "workbench.preferredDarkColorTheme": "Default Light+",
  "indentRainbow.indicatorStyle": "light",
  "indentRainbow.lightIndicatorStyleLineWidth": 5,
  // the same colors as above but more visible
  "indentRainbow.colors": [
    "rgba(255,255,64,0.3)",
    "rgba(127,255,127,0.3)",
    "rgba(255,127,255,0.3)",
    "rgba(79,236,236,0.3)"
  ],
  "workbench.colorCustomizations": {
    "[Monokai]": {
      "terminal.foreground": "#00ff62",
      "peekViewTitleLabel.foreground": "#ff0000",
      "peekViewEditor.background": "#200202",
      "list.focusBackground": "#035244",
      "editor.lineHighlightBackground": "#021a1d",
      "editor.background": "#0b192457",
      "sideBar.background": "#110c0757",
      "editor.selectionBackground": "#2a718683",
      "editor.lineHighlightBorder": "#393a6e",
      "editorGroupHeader.tabsBackground": "#3d3936",
      "editorCursor.foreground": "#0ebb50",
      "peekViewEditor.matchHighlightBackground": "#141f94",
      "tab.activeBackground": "#7a7c78a6",
      "breadcrumb.background": "#7a7c78a6",
      "editorLineNumber.activeForeground": "#88ff00e7",
      "editorLineNumber.foreground": "#00c3ff5e",
      "editor.selectionHighlightBackground": "#3331bb",
    },
  },
  "editor.tokenColorCustomizations": {
    "[Monokai]": {
      "functions": "#67cc67",
      "comments": "#a0c1c5",
      "keywords": "#ff6600",
      "numbers": "#00ff9d",
      "strings": "#aad4d1",
      "textMateRules": [
        {
          "scope": "source.python",
          "settings": {
            "foreground": "#cc12fa",
            "fontStyle": "italic bold"
          }
        },
        {
          "scope": "meta.item-access.python",
          "settings": {
            "foreground": "#ebc08fd2",
            "fontStyle": "italic bold"
          }
        },
        {
          "scope": "meta.attribute.python",
          "settings": {
            "foreground": "#ebc08fd2",
            "fontStyle": "italic bold"
          }
        },
        {
          "scope": "meta.function-call.generic.python",
          "settings": {
            "foreground": "#2af310d2",
            "fontStyle": "italic bold"
          }
        },
        {
          "scope": "meta.member.access.python",
          "settings": {
            "foreground": "#42c1f3d2",
            "fontStyle": "italic bold"
          }
        },
        {
          "scope": "meta.function-call.generic.python",
          "settings": {
            "foreground": "#d6e738fd",
            "fontStyle": "italic bold"
          }
        },
        
        {
          "scope": "entity.name.type.class.python",
          "settings": {
            "foreground": "#d6e738fd",
            "fontStyle": "italic bold"
          }
        },
        {
          "scope": "entity.other.inherited-class.python",
          "settings": {
            "foreground": "#d6e738fd",
            "fontStyle": "italic bold"
          }
        },
        
      ],
    },
  },
  "editor.snippetSuggestions": "bottom",
  "editor.suggest.showSnippets": true,
  "html.format.templating": true,
  "notebook.displayOrder": [],
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "editor.fontFamily": "Inconsolata,Consolas, 'Courier New', monospace",
  "editor.bracketPairColorization.enabled": true,
  "editor.suggest.preview": true,
  "workbench.startupEditor": "none",
  "jupyter.askForKernelRestart": false,
  "search.followSymlinks": false,
  "liveServer.settings.donotVerifyTags": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "indentRainbow.ignoreLinePatterns": [
    "/[ \t]* [*]/g",
    "/[ \t]+[/]{2}/g"
  ],
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.links": false,
  "files.trimTrailingWhitespace": true,
  "debug.terminal.clearBeforeReusing": true,
  "typescript.suggest.completeFunctionCalls": true,
  "typescript.format.placeOpenBraceOnNewLineForFunctions": true,
  "typescript.format.placeOpenBraceOnNewLineForControlBlocks": true,
  "typescript.format.insertSpaceBeforeFunctionParenthesis": true,
  "references.preferredLocation": "view",
  "indentRainbow.colorOnWhiteSpaceOnly": true,
  "html.format.indentHandlebars": true,
  "terminal.integrated.defaultProfile.windows": "Command Prompt",
  "editor.guides.bracketPairs": "active",
  "editor.renderControlCharacters": false,
  "editor.renderWhitespace": "boundary",
  "workbench.colorTheme": "Monokai",
  "editor.codeActionsOnSave": {},
  "workbench.tree.indent": 20,
  "editor.wrappingIndent": "indent",
  "workbench.tree.renderIndentGuides": "always",
  "terminal.integrated.inheritEnv": false,
  "python.condaPath": "conda acvivate py36",
  "EnglishChineseDictionary.enableHover": true,
  "editor.tabCompletion": "on",
  "editor.autoIndent": "advanced",
  "editor.unfoldOnClickAfterEndOfLine": true,
  "outline.collapseItems": "alwaysCollapse",
  "workbench.settings.useSplitJSON": true,
  "workbench.iconTheme": "material-icon-theme",
  "editor.cursorStyle": "block",
  "editor.autoClosingDelete": "always",
  "editor.autoClosingOvertype": "always",
  "editor.defaultFormatter": "ms-python.python",
  "editor.minimap.autohide": true,
  "python.languageServer": "Jedi",
  "[jsonc]": {
  "editor.defaultFormatter": "vscode.json-language-features"
  },
}
